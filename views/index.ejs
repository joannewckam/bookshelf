<script>
	$(document).ready(function () {
		$(".carousel").carousel({ indicators: true });
	});
</script>

<div class="carousel">
	<% book.forEach(function(b) { %>
	<a class="carousel-item" href="/books/<%= b._id %>">
		<img src="<%= b.bookCover %>" />
	</a>
	<% }); %>
</div>
